<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
    <rule>
        <from casesensitive="true">^/v$</from>
        <to>/Viewer.html</to>
    </rule>
    <rule>
        <from casesensitive="true">^/upload$</from>
        <to>/Upload.html</to>
    </rule>
    <rule>
        <from casesensitive="true">^/presenter$</from>
        <to>/Presenter.html</to>
    </rule>
    <rule>
        <from casesensitive="true">^/library$</from>
        <to>/Library.html</to>
    </rule>
</urlrewrite>
